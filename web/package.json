{
  "name": "web",
  "version": "0.0.7",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server-watch": "nodemon ./bin/www",
    "db": "mongod --port 5002",
    "server": "node ./bin/www",
    "client": "node start-client.js",
    "heroku-postbuild": "cd client && yarn --production=false && yarn run build",
    "dev": "concurrently \"npm run db\" \"npm run server-watch\" \"npm run client\"",
    "deploy": "publish.sh"
  },
  "dependencies": {
    "apollo-server-express": "^1.1.2",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "graphql-date": "^1.0.3",
    "graphql-tools": "^1.2.2",
    "jade": "~1.11.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.11.11",
    "morgan": "~1.8.1",
    "pcmli.umbrella.backend": "^0.1.25",
    "pcmli.umbrella.core": "^4.0.6",
    "pcmli.umbrella.uni-core": "^0.1.7",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
