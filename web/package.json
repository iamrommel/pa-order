{
  "name": "web",
  "version": "0.0.8",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server": "node ./bin/www",
    "client": "node start-client.js",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "deploy": "publish.sh"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "jade": "~1.11.0",
    "morgan": "~1.8.1",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
